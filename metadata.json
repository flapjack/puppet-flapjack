{
  "name": "flapjack-flapjack",
  "version": "0.1.0",
  "author": "Birger Schmidt, Lindsay Holmwood, Jesse Reynolds",
  "summary": "Installs, configures, and manages Flapjack.",
  "license": "MIT",
  "source": "git://github.com/flapjack/puppet-flapjack.git",
  "project_page": "https://github.com/flapjack/puppet-flapjack",
  "issues_url": "https://github.com/flapjack/puppet-flapjack/issues",
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 2.4.0 < 5.0.0"},
